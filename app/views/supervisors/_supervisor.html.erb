<div id="<%= dom_id supervisor %>">
 <tr>
      <td><%= supervisor.name %></td>
      <td><%= supervisor.surname %></td>
      <td><%= supervisor.dni %></td>
      <td><%= supervisor.contact %></td>
      <td>
          <% if supervisor.habilitado %>
           HABILITADO
          <% else %>
           NO HABILITADO
          <% end %>
     </td>
      <td>
      <a <%= link_to "Editar", edit_supervisor_path(supervisor), class:"btn btn-outline-dark" %>
      </a>
      </td>
    </tr>
</div>
